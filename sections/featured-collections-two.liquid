            {%- if section.settings.image_three.width >= 1500 -%}{{ section.settings.image_three | img_url: '1500x' }} 1500w,{%- endif -%}
            {%- if section.settings.image_three.width >= 1780 -%}{{ section.settings.image_three | img_url: '1780x' }} 1780w,{%- endif -%}
            {%- if section.settings.image_three.width >= 2000 -%}{{ section.settings.image_three | img_url: '2000x' }} 2000w,{%- endif -%}
            {%- if section.settings.image_three.width >= 3000 -%}{{ section.settings.image_three | img_url: '3000x' }} 3000w,{%- endif -%}
            {%- if section.settings.image_three.width >= 3840 -%}{{ section.settings.image_three | img_url: '3840x' }} 3840w,{%- endif -%}
            {{ section.settings.image_three | img_url: 'master' }} {{ section.settings.image_three.width }}w"
            sizes="100vw"
            src="{{ section.settings.image_three | img_url: '1000x' }}"
            loading="lazy"
            class="object-center object-cover group-hover:opacity-90 sm:absolute sm:inset-0 sm:w-full sm:h-full"
            alt="{{ section.settings.image_three.alt | escape }}"
            width="{{ section.settings.image_three.width }}"
            height="{{ section.settings.image_three.width | divided_by: section.settings.image_one.aspect_ratio }}"
          >
          
          <div aria-hidden="true" class="bg-gradient-to-b from-transparent to-black opacity-50 sm:absolute sm:inset-0"></div>
          <div class="p-6 flex items-end sm:absolute sm:inset-0">
            <div>
              <h3 class="font-semibold text-white">
                <a href="{{section.settings.button_link_three}}">
